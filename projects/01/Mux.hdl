/** 
 * Multiplexor:
 * if (sel == 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=sel, out=nsel);
    And(a=a, b=nsel, out=temp1);
    And(a=sel, b=b, out=temp2);
    Or(a=temp1, b=temp2, out=out);


}